#!/bin/execlineb -S0

` -n defaulthome
{
  && -nt
  {
    ` -n defaultuser { id -un }
    $ -u defaultuser defaultuser
    $ -D $defaultuser USER USER
    < -w 2 /dev/null
    ~ ${USER}
  }
  s6-echo /
}
$ -u defaulthome defaulthome
$ -D $defaulthome HOME HOME
tryexec { ${HOME}/.execline-shell $@ }
/bin/sh $@
